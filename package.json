{
  "name": "universal-react-boilerplate",
  "version": "3.0.0",
  "description": "A new Webpack boilerplate with React components and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build:webpack:dev": "webpack --config webpack.config.dev.js",
    "build": "npm run clean && npm run build:webpack",
    "build:dev": "npm run clean && npm run build:webpack:dev",
    "start": "cross-env NODE_PATH=source babel-node devServer.js",
    "server": "cross-env NODE_PATH=source babel-node source/server/server.js",
    "lint": "eslint source/ static/"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.7",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "cross-env": "1.0.7",
    "eslint": "2.4.0",
    "eslint-plugin-react": "4.2.2",
    "rimraf": "2.5.2",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "dependencies": {
    "express": "4.13.4",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "react-router-redux": "4.0.0",
    "redux": "3.3.1",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:justinjung04/universal-react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/justinjung04/universal-react-boilerplate/issues"
  },
  "homepage": "https://github.com/justinjung04/universal-react-boilerplate"
}
